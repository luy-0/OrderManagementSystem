<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!--    <classPathEntry location="/Program Files/IBM/SQLLIB/java/db2java.zip" />-->
    <context id="DB2Tables" targetRuntime="MyBatis3">

        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"></plugin>

        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/ordermanagement?serverTimezone=Asia/Shanghai"
                        userId="root"
                        password="224512">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <javaModelGenerator targetPackage="cn.orderManagement.bean" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper"  targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="cn.orderManagement.dao"  targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table  tableName="bill" domainObjectName="Bill" >
        </table>

        <table  tableName="complaint" domainObjectName="Complaint" >
        </table>

        <table  tableName="dish" domainObjectName="Dish" >
        </table>

        <table  tableName="employee" domainObjectName="Employee" >
        </table>

        <table  tableName="manager" domainObjectName="Manager" >
        </table>

        <table  tableName="menu" domainObjectName="Menu" >
        </table>

        <table  tableName="menu_has_dishes" domainObjectName="MenuHasDishes" >
        </table>

        <table  tableName="merchant" domainObjectName="Merchant" >
        </table>

        <table  tableName="order0" domainObjectName="Order" >
        </table>

        <table  tableName="order_has_dishes" domainObjectName="orderHasDishes" >
        </table>

    </context>
</generatorConfiguration>
